/*============================================================================
============================ SETTING SLIDE HOME ==============================
==============================================================================*/
.widget-slideshow{
  margin-bottom: 0 !important;
  @include owl-nav();
  .row{
    justify-content: flex-end;
  }
}
.heroCarousel-slider{
  .owl-carousel .owl-item img{
    width: auto;
    @media (max-width:1199px) and (min-width:992px) {
      margin: 0 auto;
      //width: 100%;
    }
  }
  .owl-nav {
    div.owl-prev, div.owl-next{height:70px;   line-height: 70px;   background:rgba(255,255,255,1);
      color: #222;
      width: 70px;
      top: 50%;
      font-size: 30px;
      &:hover{background:#222; color: #fff;}
    }
    > div.owl-prev{
      left: 30px   
    }
    > div.owl-next{
      right: 30px
    }
  }

  .owl-dots{  bottom: 25px;   line-height: 100%; #{$right}: auto;position:absolute; z-index:5;
    left: 50%;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    -o-transform: translateX(-50%);
    .owl-dot{ display: inline-block; float: $left; padding: 0 1px; width:auto; height: auto;
      > span { width: 8px; height:8px; background-color: #fbfdfd; border:none; margin:0 4px; opacity: 1; 
        display: block; @include transition(all .2s ease 0s);
      }
      &.active span{ background-color: $main-color; }
      &:hover span{background-color: $main-color;}
    }
  }

  .owl-item{
    &.active{
      .heroCarousel-content{
        .heroCarousel-subtitle{
          opacity: 1;
          animation: myeffect-slideshow 2s ease-in-out ;
        }
        .heroCarousel-title{
          opacity: 1;
          animation: myeffect-slideshow 1.5s ease-in-out ;
        }
        h3.description-tilte{
          opacity: 1;
          animation: myeffect-slideshow 1s ease-in-out ;
        }
      }
    }
  }
  .heroCarousel-content{
    transform: none; background: none;  top: 50%; padding:0 50px;
    position:absolute; z-index:5;
    text-align: center;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    width: 100%;
    @media (max-width: 768px){
      padding:0 30px;
    }
    .heroCarousel-subtitle{
      width: auto;
      opacity: 0;
      font-size: 22px; 
      font-weight: 400;
      color: #fff;
      text-align: center;
      @media (max-width: 991px){
        font-size: 15px;
      }
      @media (max-width: 768px){
        font-size: 13px;
      }
      @media (max-width: 567px){
        font-size: 12px;
      }

    }
    .heroCarousel-title{
      width: auto;
      opacity: 0;
      font-size: 38px; 
      font-weight: 400;
      color: #fff;
      text-transform: uppercase;
      @media (max-width: 991px){
        font-size: 25px;
      }
      @media (max-width: 768px){
        font-size: 20px;
      }
      @media (max-width: 567px){
        font-size: 14px;

      }

    }
    .heroCarousel-description{
      color: #fff;
    }
    h3.description-tilte{ 
      opacity: 0;
      font-size: 20px; color: white; font-weight: 400; margin: 0 0 15px; 
      span{
        font-size: 26px;
        font-weight: 600;
        color: $main-color;
        @media (max-width: 768px){
          font-size: 15px;
        }
      }
      @media (max-width: 991px){
        font-size: 15px;
      }
      @media (max-width: 768px){
        font-size: 14px;
      }
      @media (max-width: 567px){
        font-size: 12px;

      }

    }

  }
}

//====== EFFECT - SLIDERHOME  ====
@keyframes myeffect-slideshow {
  0%   {
    opacity: 0;
    transform: translateY(-300px);
    -webkit-transform: translateY(-300px);
    -moz-transform: translateY(-300px);
    -ms-transform: translateY(-300px);
    -o-transform: translateY(-300px);
  }
  100% {
    opacity: 1;
    transform: translateY(0);
    -moz-transform: translateY(0);
    -webkit-transform: translateY(0);
    -ms-transform: translateY(0);
    -o-transform: translateY(0);
  }


}


