/*============================================================================
========================= SECCTION COLLECTION ================================
==============================================================================*/

.clearfix{
  clear: both; 
}

.widget-collection {
  margin-top: 55px;
  margin-bottom: 65px;

  @media (max-width: 480px){
    margin-top: 20px;
  }
  .home-title{
    border-bottom: 1px #eee solid;
    margin-bottom: 20px;
    overflow: hidden;
    h3{
      margin-bottom: 10px;
      float: $left;
    }
    .view-all{
      float: $right;
      font-size: 13px;
      text-transform: capitalize;
      padding-top: 5px;
      -webkit-transition: all 0.3s;
      -moz-transition: all 0.3s;
      -o-transition: all 0.3s;
      -ms-transition: all 0.3s;
      transition: all 0.3s;
      &:hover{
        color: $main-color;
      }
      @media (max-width: 480px){
        float: $left;
      }
    }
  }
  .collections{
    margin: 0;
    position: relative;
    .ss-carousel{
      margin: 0 -10px;

      .owl-carousel{
        padding: 10px;
        overflow: hidden;
        padding-bottom:0;
        .owl-stage-outer{
          overflow: visible;
        }
      }
    }
    .collect{
      overflow: hidden;
      border: 1px solid #eee;
      border: 1px #eee solid;
      box-shadow: 0 0 4px 2px transparent;
      padding: 30px 10px;
      margin-bottom: 30px;
      -webkit-transition: all 0.3s;
      -moz-transition: all 0.3s;
      -o-transition: all 0.3s;
      -ms-transition: all 0.3s;
      transition: all 0.3s;
      &:hover{
        border-color: transparent;
        box-shadow: 0 0 4px 2px #ebebeb;
        -webkit-transition: all 0.3s;
        -moz-transition: all 0.3s;
        -o-transition: all 0.3s;
        -ms-transition: all 0.3s;
        transition: all 0.3s;
        @include transition(all 0.2s ease);
        img {
          opacity: 1; 
          @include transition(all 0.2s ease);
        }
      }
      a {
        display: block;
      }
      img {
        display: inline-block;
        width: auto;
        max-width: 100%;
      }
      .collection-name{
        clear: both;
        display: block;
        font-size: 14px;
        text-transform: uppercase;
        font-weight: 700;
        padding-top: 15px;
      }
    }
  }
  .item-image{
    overflow: hidden;
    width: 50%;
    float: $left;

    img{
      @include transition(all 0.2s linear);
    }
    &:hover{
      img{
        opacity: 0.8;
        filter: alpha(opacity=80);
        box-shadow: #000 0em 0em 0em;
        -webkit-transform: scale(1.1, 1.1);
        -moz-transform: scale(1.1, 1.1);
        -o-transform: scale(1.1, 1.1);
        -ms-transform: scale(1.1, 1.1);
        transform: scale(1.1, 1.1);
      }
    }
  }
  .cat-content{
    overflow: hidden;
    .colection-name{
      position: relative;
      margin-bottom: 10px;
      padding-top: 5px;
      font-weight: 600;
      font-size: 14px;
      color: #282828;
      text-transform: uppercase;

    }
    .colections{
      li{
        position: relative;
        padding-#{$left}: 10px;
        &:before{
          content: '\f111';
          font-family: FontAwesome;
          font-size: 4px;
          position: absolute;
          top: 2px;
          #{$left}: 1px;
        }
        a{
          font-size: 13px;
          color: #888888;
          font-weight: 400;
          overflow: hidden;
          position: relative;
          line-height: 26px;
          &:hover{
            color: $main-color;
          }
        }
      }
      .view-all{
        padding-left: 0;
        margin-top: 10px;
        a{
          height: 30px;
          line-height: 30px;
          padding: 0 15px;
          background: #282828;
          color: #fff;
          border-radius: 4px;
          display: inline-block;
          vertical-align: top;
          font-size: 12px;
          &:hover{
            padding: 0 15px;
            background: #ff2d37;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
            -o-transition: all 0.3s;
            -ms-transition: all 0.3s;
            transition: all 0.3s;
            color: #fff;
          }
        }
      }
    }
  }
}
