@mixin owl-nav(){
  .owl-nav{ 
    div{
      width: 50px;
      height: 50px;
      line-height: 49px;
      background-color: rgba(0,0,0,.05);
      -webkit-transition: all 0.3s ease-in-out 0s;
      -moz-transition: all 0.3s ease-in-out 0s;
      transition: all 0.3s ease-in-out 0s;
      top: 50%;
      text-align: center;
      @include transform(translateY(-50%));
      position: absolute;
      color: #fff;
      border-radius: 50%;
      font-size: 24px;
      color: #fff;
      opacity: 0;

      &:hover{background: #222;}  
      &.owl-prev{
        #{$left}: -25px;
        #{$right}: inherit;		
        @include if-rtl{
          float: left; 
          transform: rotate(180deg);
          -webkit-transform: rotate(180deg);
          -ms-transform: rotate(180deg);
          -o-transform: rotate(180deg);
        }
      }
      &.owl-next{
        #{$right}: -25px;
        #{$left}: inherit;
        @include if-rtl{
          transform: rotate(180deg);
          -webkit-transform: rotate(180deg);
          -ms-transform: rotate(180deg);
          -o-transform: rotate(180deg);
        }
      } 
      @media (min-width: 1200px) and (max-width: 1229px){
        width: 30px;
        height: 30px;
        line-height: 30px;
        font-size: 18px;
        &.owl-prev{
          #{$left}: -15px;
        }
        &.owl-next{
          #{$right}: -15px;
        }
      }
      @media (max-width: 768px){
        width: 30px;
        height: 30px;
        line-height: 30px;
        font-size: 18px;
        &.owl-prev{
          #{$left}: -15px;
        }
        &.owl-next{
          #{$right}: -15px;
        }
      }
    }    
  }
  &:hover{
    .owl-nav{ 
      div{
        opacity: 1;
      }
    }
  }
}
@mixin owl-controls(){
  .owl-nav {
    div.owl-prev, div.owl-next{
      top: -65px;
      position: absolute;
      height: 30px;
      width: 30px;
      line-height: 30px;
      border: none;
      opacity: 1;
      background: #c1c1c1;
      font-size: 18px;
      margin: 0;
      padding: 0;
      color: #222222;
      text-align: center;
      @include transition (all 0.2s ease-in-out 0s);
      @media (max-width: 568px){
        top: -55px;
        height: 24px;
        width: 24px;
        line-height: 24px;
      }
      &:before{
        display: none;
      }
      &:hover{
        color: #fff;
        background-color: $main-color;
      }
    }
    div.owl-prev{
      #{$left}: auto; 
      #{$right}: 32px;
      &:hover {
        background-position: 0px 0;
      }

    }
    div.owl-next{
      #{$left}: auto; 
      #{$right}: 0;
    }

  }
}
