/*============================================================================
========================= Widget Product Tabs     ============================
==============================================================================*/
.widget-product-tabs{
  margin-bottom: 0 !important;
  padding-bottom: 60px ;
  .owl-carousel .owl-stage{
    display: flex;
    .owl-item{
      display: inline-flex;
    }
  }
  @media (max-width: 768px){
    padding-bottom: 20px;
  }
  .layout-image{
    width: 280px;
    float: $left;
    @media (max-width: 991px){
      display: none;
      width: 250px;
    }
    @media (max-width: 767px){
      display: none;
    }
  }
  .layout-space{
    padding: 0 20px;
    @media (max-width: 991px){
      padding-bottom: 20px;
    }
  }
  .ltabs-tabs-container{
    overflow: hidden;
  }
  .element-loading > .ltabs-products-loader{
    opacity: 1;
    visibility: visible;
    display:block;
  }
  .ltabs-products-loader{
    position: absolute;
    left: 50%;
    top: 40%;
    margin-left: -17px;
    opacity: 0;
    visibility: hidden;
    width: 60px;
    height: 60px;
    img{width:100%;}
  }
  .widget-content{
    overflow: hidden;
    background: #fff;
    .owl-carousel .owl-stage-outer{
      // overflow: visible;
    }
  }
  .ltabs-tabs-container{
    position: relative;
    .tabs-menu{
      position: relative;
      @media (max-width:991px){
        padding: 15px 0;
      }
      @media (min-width: 569px){

        ul{
          overflow: hidden;
        }
        .tabs-menu_btn{
          display : none;
        }
        li.tabs-menu-label{
          position: relative; 
        }
      }
      @media (max-width:568px){
        .tabs-menu_btn {
          display : block;
          //display : none;
          border: 1px solid #ddd;
          font-size: 21px;
          color: #222;
          height: 30px;
          width: 33px;
          text-align: center;
          line-height: 28px;
          margin-top: 10px;
          margin-bottom: 10px;
        }
      }
      .tabs-menu_title{
        overflow: hidden;
        li{
          float: $left;
          font-size: 12px;
          position: relative;
          padding: side-values(0 40px 0 0px);
          cursor: pointer;
          >span{
            display: block;
            padding: 20px 0;
            position: relative;
            border: none;
            font-size: 16px;
            text-transform: uppercase;
            font-weight: 600;
            color: #222;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
            -o-transition: all 0.3s;
            -ms-transition: all 0.3s;
            transition: all 0.3s;
            @media (max-width: 991px){
              padding: 0;
            }
            @media (min-width: 568px){
              line-height: 36px;
            }
            @include transition(all 200ms ease-in-out 0s);
          }
          &:hover,
          &.current{
            >span{
              color: $main-color !important;

              @media (min-width: 992px){
                &:after{
                  opacity: 1;
                  visibility: visible;
                }
              }
            }
          }
        }

        @media (max-width: 568px){
          display:none;
          margin: 0;
          max-width: 200px;
          min-width: 120px;
          background: #fff;
          border: 1px solid #ddd;
          z-index: 10;
          padding: 10px 20px;
          position: absolute;
          #{$left}: 0;
          top: calc(100% + -1px);
          border-bottom-right-radius: 3px;
          border-bottom-left-radius: 3px;
          margin-bottom: 10px;
          &.tabs-open{
            display: block; 
          }
          >li{
            display: block;
            padding: 8px 0;
            text-align: $left;
            width: 100%;
            border: none!important;
            background: transparent!important;
            border-bottom: 1px solid #ddd!important;
            &:last-child{ border: none!important;}
            >span{ color: #222!important; line-height: 20px;}
          }

        } 
      }
    }
    .tabs-content {
      position: relative;
      transition: transform 0.2s ease-in-out, opacity 0.2s ease-in-out, visibility 0.2s ease-in-out;
      transform: translateY(0px);

      &.loading {
        opacity: 0;
        visibility: hidden;
        transform: translateY(20px);
        .owl-nav, .owl-dots {display: none;}
      }  
    }

  }

}

